{
  "version":2,
  "builds": [
    {
      "src": "bot_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bot_server.py"
    }
  ],
  "crons": [
    {
      "path": "post-now?type=meme&api_key=supersecretapikey",
      "schedule": "*/10 * * * *"
    }
  ]
}